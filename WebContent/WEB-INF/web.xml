<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd" id="WebApp_ID" version="4.0">
  <display-name>UserManagement</display-name>
  <welcome-file-list>
    <welcome-file>LandingPage.jsp</welcome-file>
  </welcome-file-list>
    <servlet>
	<servlet-name>login</servlet-name>
	<servlet-class>com.xadmin.usermanagement.web.login</servlet-class>
 	</servlet>
	<servlet-mapping>
	<servlet-name>login</servlet-name>
	<url-pattern>/login</url-pattern>
	</servlet-mapping>
	
  	<servlet>
	<servlet-name>Dashboard</servlet-name>
	<servlet-class>com.xadmin.usermanagement.web.Dashboard</servlet-class>
 	</servlet>
	<servlet-mapping>
	<servlet-name>Dashboard</servlet-name>
	<url-pattern>/dashboard</url-pattern>
	</servlet-mapping>
  <error-page>
  <exception-type>java.lang.Exeption</exception-type>
  <location>/Error.jsp</location>
  </error-page>
</web-app>